@{
  Layout = "_Layout";
}
<h1>Order Details:</h1>

<p>Order title: @Model["order"].Title</p>
<p>Order description: @Model["order"].Description</p>
<p>Order price: @Model["order"].Price</p>
<p>Order date: @Model["order"].Date</p>
<p>Vendor: @Model["vendor"].Name</p>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new order</a></p>
<p><a href='/vendors'>Return to vendor list</a></p>
<p><a href='/'>Return to Main Page</a></p>